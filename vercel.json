{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  
  "functions": {
    "api/autopilot.ts": {
      "runtime": "nodejs20.x"
    },
    "api/kvHealth.ts": {
      "runtime": "nodejs20.x"
    },
    "api/kv-health/index.ts": {
      "runtime": "nodejs20.x"
    },
    "api/kv.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
    ,
    {
      "source": "/api/kv-health",
      "destination": "/api/kvHealth"
    }
  ]
}