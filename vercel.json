{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "AUTOPILOT_ENABLED": "true",
    "ARKAIOS_API_KEY": "@arkaios_api_key"
  },
  "functions": {
    "api/autopilot.ts": {
      "runtime": "@vercel/node"
    },
    "api/kv-health.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}